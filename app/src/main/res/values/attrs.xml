<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="clockBackground" format="color"/>
    <attr name="clockTextColor" format="color"/>

    <declare-styleable name="ViewPagerIndicator">
        <attr name="titleCount" format="integer"/>
    </declare-styleable>

    <declare-styleable name="NavItem">
        <attr name="startIcon" format="reference"/>
        <attr name="action" format="string"/>
        <attr name="textColor" format="color"/>
        <attr name="actionState" format="string"/>
        <attr name="tint" format="color"/>
    </declare-styleable>

    <declare-styleable name="AddWidget">
        <attr name="circle_anim" format="boolean"/>
        <attr name="sub_anim" format="boolean"/>
    </declare-styleable>
</resources>